package commom.com;

public class Student {
	
	String firstName;
	String lastName;
	int SSN;
	public Student(String firstName ,String lastName , int SSN) {
			
		this.firstName = firstName;
		this.lastName = lastName ;
		this.SSN = SSN
		
	}
	
		
	// student   -- firstName lastName SSN
	
	// teacher  --  firstName lastName SSN , salary
	
	// then become professor ---  firstName lastName SSN , salary , spec.
	

}
