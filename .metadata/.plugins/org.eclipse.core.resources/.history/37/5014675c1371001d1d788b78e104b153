package commom.com;

public class Student {

	String firstName;
	String lastName;
	int SSN;

	public Student(String firstName, String lastName, int SSN) {
		this.firstName = firstName;
		this.lastName = lastName;
		this.SSN = SSN;
	}

	public void displayName() {
		System.out.println(this.firstName + this.lastName);
	}
	
}

